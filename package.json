{
  "name": "waterfront",
  "version": "0.0.1",
  "description": "For the store",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:public": "cd ./public && npm start",
    "start:server": "cd ./server && npm start",
    "setup:public": "cd ./public && npm i",
    "setup:server": "cd ./server && npm i",
    "postinstall": "npm run setup:server && npm run setup:public",
    "prod:build": "cd ../public && npm run build:prod",
    "prod:start": "cd ../server && npm run start:prod"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  }
}
